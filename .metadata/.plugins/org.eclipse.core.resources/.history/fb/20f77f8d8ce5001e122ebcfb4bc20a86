
public class EJE1 {

	public static void main(String[] args) {
		int a = 1;
		int b= 7;	
		imprimirConFor(a,b);
		System.out.println('-------------------');
		imprimirConWhile(a,b);
	}
	
	public static void imprimirConFor(int a, int b) {
		for (int i = a; i<=b;i++) {
			System.out.println(i);
		}
	}


	public static void imprimirConWhile(int a,int b) {
		int i = a;
		while (i<=b) {
			System.out.println(i);
			i++;
		}
	}
	
	
	
}